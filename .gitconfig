[user]
	email = ericgclifford@gmail.com
	name = Eric Clifford
[push]
	default = simple
[core]
	editor = nvim
	excludesfile = /Users/eclifford/.gitignore
  attributesfile = ~/.gitattributes
[git-p4]
   attemptRCSCleanup = true
[credential]
	helper = osxkeychain
[github]
	user = eclifford
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[rerere]
	enabled = 1
